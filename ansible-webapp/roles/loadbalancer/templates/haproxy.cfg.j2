global
    log /dev/log    local0
    maxconn 2000

defaults
    log     global
    option  httplog
    timeout connect 5000ms
    timeout client  50000ms
    timeout server  50000ms

frontend http_front
    bind *:80
    default_backend http_back

backend http_back
    server webserver1 192.168.0.194:80 check
